{
  "name": "tec.sandbox.web.client",
  "version": "0.0.0",
  "engines": {
    "node": ">=21.6.1",
    "npm": "10.8.2"
  },
  "contributors": [
    "J. Navarrete <t-josenavarrete@itesm.mx>"
  ],
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "build": "ng build",
    "build:production": "ng build --configuration=production",
    "build:staging": "ng build --configuration=staging",
    "build:devl": "ng build --configuration=devl",
    "build:development": "ng build --configuration=development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prestart": "node aspnetcore-https",
    "start:windows": "ng serve --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng serve --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1",
    "lint": "ng lint --fix",
    "format": "npx prettier --write src/app/* --ignore-unknown",
    "standards:check": "node .devlocal/automation/project-scripts/standards-check.js",
    "standards:fix": "node .devlocal/automation/project-scripts/standards-fix.js && npm run lint && npm run format",
    "update-docs": "bash .devlocal/automation/project-scripts/update-documentation.sh",
    "update-context": "bash .devlocal/automation/project-scripts/update-context.sh",
    "daily-workflow": "bash .devlocal/automation/project-scripts/daily-workflow-complete.sh",
    "project-analysis": "bash .devlocal/automation/project-scripts/daily-workflow-complete.sh deep-context",
    "generate-bitacora": "python3 .devlocal/archive/automatizador_bitacora.py",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "generate-sonar-properties": "node generate-sonar-project-properties.js",
    "sonar": "npm run generate-sonar-properties && sonar-scanner",
    "print-env": "dotenv -e .env printenv | grep SONAR",
    "refine-hu": "bash .devlocal/automation/refine-hu.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "^19.2.18",
    "@angular/common": "^19.2.14",
    "@angular/compiler": "^19.2.14",
    "@angular/core": "^19.2.14",
    "@angular/forms": "^19.2.14",
    "@angular/material": "^19.2.18",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/router": "^19.2.14",
    "@ngrx/signals": "^19.2.1",
    "@ngrx/store": "^19.2.1",
    "@ngrx/store-devtools": "^18.0.2",
    "@ti-tecnologico-de-monterrey-oficial/ds-ng": "^1.5.819-e",
    "chart.js": "^4.4.3",
    "dotenv": "^16.4.5",
    "jest-editor-support": "*",
    "jwt-decode": "^4.0.0",
    "material-icons": "^1.13.12",
    "material-symbols": "^0.17.2",
    "ng2-charts": "^6.0.1",
    "ngx-cookie-service": "^19.0.0",
    "run-script-os": "*",
    "rxjs": "~7.8.0",
    "rxjs-compat": "^6.6.7",
    "sass": "^1.75.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.14",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/schematics": "18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "^19.2.14",
    "@angular/compiler-cli": "^19.2.14",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@ngrx/eslint-plugin": "^18.0.2",
    "@ngrx/store-devtools": "^19.2.1",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "autoprefixer": "^10.4.19",
    "dotenv-cli": "^7.4.2",
    "eslint": "8.57.0",
    "globals": "^15.9.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.38",
    "prettier": "3.3.3",
    "sonarqube-scanner": "^4.2.3",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.4.0"
  }
}